<?xml version="1.0" encoding="UTF-8"?>
<project name="neard-dev-misc" basedir=".">
  
  <fail unless="project.basedir" message="Property 'project.basedir' must be setted before loading build-bundle.xml"/>
  <fail unless="neard-dev.path" message="Property 'neard-dev.path' must be setted before loading build-bundle.xml"/>
      
  <property name="build-misc.properties" value="${neard-dev.path}/build/build-misc.properties"/>
  <echo message="Load build-misc.properties: ${neard-dev.path}/build/build-misc.properties" level="debug"/>
  <property file="${build-misc.properties}"/>
  
  <!-- Import build-commons.xml -->
  <import file="build-commons.xml"/>
  
  <!-- Properties -->
  <property name="build.misc.path" value="${build.path}/misc"/>
  
</project>
